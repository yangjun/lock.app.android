<manifest
    package="cn.finalteam.galleryfinal"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <!--在SDCard中创建与删除文件权限-->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <!--往SDCard写入数据权限-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <!--从SDCard读取数据权限-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <!--网络权限-->
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:supportsRtl="true">

        <activity
            android:name=".ImageSelectActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ImageEditActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ImagePreviewActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ImageTakePhotoActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ImageGuideActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ImageClipActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="cn.finalteam.demo.ImageDemoActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>